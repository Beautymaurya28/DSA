//best time to buy and sell product
class Solution {
public:
    int maxProfit(vector<int>& prices) {
       int minprice=INT_MAX;
       int maxprofit=0;
       //min price
       for(int i=0;i<prices.size();i++){
        if(prices[i]<minprice){
            minprice=prices[i];
        }
        //profit(max profit)
        int profit=prices[i]-minprice;
        maxprofit=max(maxprofit,profit);
       }
       return maxprofit;
    }
};
